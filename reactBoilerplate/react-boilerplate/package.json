    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "babel-loader": "^9.1.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.55.0",
