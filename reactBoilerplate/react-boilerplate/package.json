{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "build:client": "babel src/client --out-dir dist/client --presets @babel/preset-react",
    "build:server": "babel src/server --out-dir dist/server --presets @babel/preset-env",
    "start": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "dev": "webpack serve --config webpack.config.js --mode development --open"
